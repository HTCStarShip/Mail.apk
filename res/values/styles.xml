<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="readscreen_label" parent="@com.htc:style/title_primary_xs">
        <item name="android:gravity">left|center</item>
        <item name="android:layout_gravity">left|center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="pure_divider">
        <item name="android:background">@com.htc:drawable/common_list_divider</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">2.0px</item>
    </style>
    <style name="section_divider">
        <item name="android:background">@com.htc:drawable/section_divider</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">2.0px</item>
    </style>
    <style name="ec_account_label" parent="@com.htc:style/list_primary_xs">
        <item name="android:gravity">left</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/ec_margin_horizontal</item>
        <item name="android:layout_marginTop">@dimen/ec_margin_vertical_label_top</item>
        <item name="android:layout_marginRight">@dimen/ec_margin_horizontal</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="ec_account_field" parent="@com.htc:style/input_default_s">
        <item name="android:background">?com.htc:attr_common_inputfield_light</item>
        <item name="android:paddingLeft">@dimen/ec_edit_field_padding_left</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/ec_margin_horizontal</item>
        <item name="android:layout_marginTop">@dimen/ec_margin_vertical_label_top</item>
        <item name="android:layout_marginRight">@dimen/ec_margin_horizontal</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="ec_account_field_spinner" parent="@com.htc:style/list_primary_s">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/ec_margin_horizontal_s</item>
        <item name="android:layout_marginRight">@dimen/ec_margin_horizontal_s</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="ec_checkbox" parent="@com.htc:style/list_primary_xs">
        <item name="android:gravity">left|center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/ec_margin_horizontal_checkbox</item>
        <item name="android:layout_marginTop">@dimen/ec_margin_vertical_checkbox</item>
        <item name="android:layout_marginBottom">@dimen/ec_margin_vertical_checkbox</item>
        <item name="android:button">?com.htc:attr_common_checkbox</item>
    </style>
    <style name="ex_single_button" parent="@com.htc:style/list_primary_m">
        <item name="android:background">?com.htc:attr_common_button_small</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/ex_margin_horizontal</item>
        <item name="android:layout_marginRight">@dimen/ex_margin_horizontal</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="ex_account_label" parent="@com.htc:style/list_primary_xs">
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/ex_margin_horizontal</item>
        <item name="android:layout_marginRight">@dimen/ex_margin_horizontal</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="ex_account_field" parent="@com.htc:style/list_primary_s">
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">?com.htc:attr_common_inputfield_light</item>
        <item name="android:paddingLeft">@dimen/ec_edit_field_padding_left</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/ex_margin_horizontal</item>
        <item name="android:layout_marginTop">@dimen/ex_margin_vertical_label_top</item>
        <item name="android:layout_marginRight">@dimen/ex_margin_horizontal</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="ex_spinner" parent="@com.htc:style/list_primary_s">
        <item name="android:padding">0.0px</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/ex_margin_horizontal_s</item>
        <item name="android:layout_marginRight">@dimen/ex_margin_horizontal_s</item>
    </style>
    <style name="ex_checkbox">
        <item name="android:textSize">@com.htc:dimen/list_primary_xs</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">left|center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/ex_margin_horizontal_s</item>
        <item name="android:layout_marginRight">@dimen/ex_margin_horizontal_s</item>
        <item name="android:button">?com.htc:attr_common_checkbox</item>
    </style>
    <style name="top_round_small">
        <item name="android:background">@color/list_bg</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@com.htc:drawable/trans_topround</item>
        <item name="android:scaleType">fitXY</item>
    </style>
    <style name="bottom_round_small">
        <item name="android:background">@com.htc:color/white</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@com.htc:drawable/trans_bottomround</item>
        <item name="android:scaleType">fitXY</item>
    </style>
    <style name="trans_topround">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@com.htc:drawable/trans_topround</item>
        <item name="android:scaleType">fitXY</item>
    </style>
    <style name="trans_bottomround">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@com.htc:drawable/trans_bottomround</item>
        <item name="android:scaleType">fitXY</item>
    </style>
    <style name="bottomround_v1">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@com.htc:drawable/bottomround_v1</item>
        <item name="android:scaleType">fitXY</item>
    </style>
    <style name="topround_v1">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@com.htc:integer/listview_border_height</item>
        <item name="android:src">@com.htc:drawable/topround_v1</item>
        <item name="android:scaleType">fitXY</item>
    </style>
    <style name="line_divider">
        <item name="android:background">#ffbdbdbd</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1.0px</item>
    </style>
    <style name="ThemePreview">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="com.htc:fakescreenStyle">style_04</item>
    </style>
    <style name="windowNullContentOverlay">
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="no_window_content_overlay">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@drawable/transparent</item>
    </style>
    <style name="list_noTitleBar">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="list_Translucent">
        <item name="android:colorForeground">#ffff0000</item>
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
    <style name="Theme.Translucent" parent="@*android:style/Theme.Translucent">
        <item name="android:colorForeground">#ffff0000</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="FindDialogTheme">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:background">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="FloatingDialogTheme">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
    <style name="FloatingHideTheme">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
</resources>
